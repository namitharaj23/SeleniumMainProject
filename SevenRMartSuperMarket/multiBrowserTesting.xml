<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="multiBrowserTest" parallel="tests" thread-count="5">


	<test name="chromeTest">

		<parameter name="browzer" value="chrome"></parameter>

		<classes>

			<class name="testscript.LoginTest">
				<methods>
					<include
						name="verifyUserIsUnableToLoginWithValidPasswordAndInalidUserName"></include>
				</methods>
			</class>

		</classes>
	</test> 
	<test name="edgeTest">

		<parameter name="browzer" value="edge"></parameter>

		<classes>

			<class name="testscript.LoginTest">
				<methods>
					<include
						name="verifyUserIsUnableToLoginWithValidPasswordAndInalidUserName"></include>
				</methods>
			</class>

		</classes>
	</test> <!--
	TestNG -->
</suite> <!--
Suite -->